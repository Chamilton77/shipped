<h1>Boats#new</h1>
<p>Find me in app/views/boats/new.html.erb</p>

<%= form_for @boat do |f| %>
	<%= f.label :name, "Boat Name" %>
	<%= f.text_field :name %>
	<%= f.label :containters, "Number of Containters" %>
	<%= f.text_field :containers %>
	<%= f.label :location, "Location" %>
	<%= f.text_field :location %>
	<%= f.submit "Submit" %>
<% end %>
  